{
  "name": "rss-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "0.17.1",
    "bloomer": "^0.6.3",
    "bulma": "^0.6.2",
    "chart.js": "^1.1.1",
    "jquery": "3.3.1",
    "node-sass-chokidar": "0.0.3",
    "react": "^16.2.0",
    "react-chartjs": "^1.2.0",
    "react-dom": "^16.2.0",
    "react-scripts": "1.1.0",
    "react-swipeable-views": "^0.12.12",
    "rss-parser": "^3.0.1",
    "semiotic": "1.7.9"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/components/_styles/scss -o src/components/_styles/css",
    "watch-css": "npm run build-css && node-sass-chokidar src/components/_styles/scss -o src/components/_styles/css --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
